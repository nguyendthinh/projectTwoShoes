<h2>Edit The Sneaker</h2>
<%= form_for @sneaker do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.label :shoesize %>
  <%= f.text_field :shoesize %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.submit "Submit" %>
<% end %>
